{
  "uid": "marketalert_fastapi",
  "title": "Métricas FastAPI – MarketAlert",
  "schemaVersion": 39,
  "version": 5,
  "refresh": "5s",
  "timezone": "",
  "panels": [
    {
      "id": 1,
      "title": "Requisições HTTP Totais",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 6 },
      "targets": [
        {
          "expr": "sum(http_requests_total{endpoint!='/metrics',endpoint!='/audit/metrica'})",
          "legendFormat": "Total de requisições",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": true }
      }
    },
    {
      "id": 2,
      "title": "Requisições por Método & Endpoint",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 6, "y": 0, "w": 9, "h": 6 },
      "targets": [
        {
          "expr": "sum by(method,endpoint)(http_requests_total{endpoint!='/metrics',endpoint!='/audit/metrica'})",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": true }
      }
    },
    {
      "id": 3,
      "title": "Duração P95 da Requisição",
      "type": "bargauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 15, "y": 0, "w": 9, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "Endpoint: {{endpoint}}",
          "refId": "A"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 4,
      "title": "Taxa de Requisições (req/s)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{endpoint!='/metrics',endpoint!='/audit/metrica'}[1m])) by (endpoint)",
          "legendFormat": "Endpoint: {{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },
    {
       "id": 5,
      "title": "Taxa de Erros (4xx / 5xx)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (status_code)(rate(http_requests_total{status_code=~\"4..|5..\"}[5m]))",
          "legendFormat": "Código de erro: {{status_code}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected",
          "min": 0,
          "max": 5,
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 2, "color": "orange" },
              { "value": 5, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 6,
      "title": "Latência Percentil 99",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 13, "w": 24, "h": 7 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "Endpoint: {{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },

    {
      "id": 7,
      "title": "Total /metrics",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 21, "w": 6, "h": 5 },
      "targets": [
        {
          "expr": "sum(http_requests_total{endpoint='/metrics'})",
          "legendFormat": "Requisições /metrics",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": true }
      }
    },
    {
      "id": 8,
      "title": "Taxa /metrics (req/s)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 6, "y": 21, "w": 9, "h": 5 },
      "targets": [
        {
          "expr": "rate(http_requests_total{endpoint='/metrics'}[1m])",
          "legendFormat": "req/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": false }
      }
    },
    {
      "id": 9,
      "title": "P95 /metrics",
      "type": "bargauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 15, "y": 21, "w": 9, "h": 5 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_latency_seconds_bucket{endpoint='/metrics'}[5m])) by (le))",
          "legendFormat": "latência",
          "refId": "A"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },

    {
      "id": 10,
      "title": "Total /audit/metrica",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 27, "w": 6, "h": 5 },
      "targets": [
        {
          "expr": "sum(http_requests_total{endpoint='/audit/metrica'})",
          "legendFormat": "Requisições /audit/metrica",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": true }
      }
    },
    {
      "id": 11,
      "title": "Taxa /audit/metrica (req/s)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 6, "y": 27, "w": 9, "h": 5 },
      "targets": [
        {
          "expr": "rate(http_requests_total{endpoint='/audit/metrica'}[1m])",
          "legendFormat": "req/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": false }
      }
    },
    {
      "id": 12,
      "title": "P95 /audit/metrica",
      "type": "bargauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 15, "y": 27, "w": 9, "h": 5 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_latency_seconds_bucket{endpoint='/audit/metrica'}[5m])) by (le))",
          "legendFormat": "latência",
          "refId": "A"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    }
  ],
  "time": { "from": "now-1h", "to": "now" },
  "annotations": {
    "list": [
      {
        "name": "Anotações & Alertas",
        "type": "dashboard",
        "enable": true,
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "grafana" }
      }
    ]
  },
  "tags": ["fastapi", "marketalert"],
  "templating": { "list": [] },
  "links": []
}
