{
  "uid": "marketalert_process",
  "title": "Métricas de Processo - MarketAlert",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "10s",
  "timezone": "",
  "panels": [
    {
      "id": 1,
      "title": "Consumo de CPU (cores)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[1m])",
          "legendFormat": "Cores usados",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "cores",
          "nullValueMode": "connected",
          "min": 0,
          "max": 4,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 1, "color": "orange" },
              { "value": 2, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 2,
      "title": "Uso de Memória Resident",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes / 1024 / 1024",
          "legendFormat": "RSS (MB)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "megabytes",
          "nullValueMode": "connected",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 512, "color": "orange" },
              { "value": 768, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value"
      }
    },
    {
      "id": 3,
      "title": "Uso de Memória Virtual",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "process_virtual_memory_bytes / 1024 / 1024",
          "legendFormat": "VMS (MB)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "megabytes",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 512, "color": "orange" },
              { "value": 768, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value"
      }
    },
    {
      "id": 4,
      "title": "Tempo de Início do Processo",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 6 },
      "targets": [
        {
          "expr": "max(process_start_time_seconds * 1000)",
          "legendFormat": "Início",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "dateTimeAsLocal"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value"
      }
    },
    {
      "id": 5,
      "title": "Distribuição de Memória",
      "type": "piechart",
      "datasource": "Prometheus",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 6 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes / 1024 / 1024",
          "legendFormat": "RSS (MB)",
          "refId": "A"
        },
        {
          "expr": "process_virtual_memory_bytes / 1024 / 1024",
          "legendFormat": "VMS (MB)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "megabytes"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "VMS (MB)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "RSS (MB)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "pieType": "pie",
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    }
  ],
  "time": { "from": "now-1h", "to": "now" },
  "annotations": { "list": [] },
  "tags": ["process", "marketalert"],
  "templating": { "list": [] },
  "links": []
}
