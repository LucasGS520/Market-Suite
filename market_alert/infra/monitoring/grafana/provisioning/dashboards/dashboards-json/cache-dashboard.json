{
  "uid": "marketalert_cache_endpoint",
  "title": "MÃ©tricas de Cache - MarketAlert",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "timezone": "",
  "panels": [
    {
     "id": 1,
      "title": "Hits de Cache por Endpoint (min)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(cache_hits_endpoint_total[1m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 2,
      "title": "Falta de Cache por Endpoint (min)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(cache_misses_endpoint_total[1m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 3,
      "title": "Cache Hits por Minuto",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 6, "y": 6, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(cache_hits_total[1m]))",
          "legendFormat": "Hits/min",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": true }
      }
    },
    {
      "id": 4,
      "title": "Falta de Cache por Minuto",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 6, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(cache_misses_total[1m]))",
          "legendFormat": "Misses/min",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": true }
      }
    }

  ],
  "time": { "from": "now-1h", "to": "now" },
  "annotations": { "list": [] },
  "tags": ["cache", "marketalert"],
  "templating": { "list": [] },
  "links": []
