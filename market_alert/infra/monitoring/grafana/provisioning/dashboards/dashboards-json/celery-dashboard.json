{
  "uid": "marketalert_celery",
  "title": "Métricas Celery - MarketAlert",
  "schemaVersion": 39,
  "version": 5,
  "refresh": "10s",
  "timezone": "",
  "panels": [
    {
      "id": 1,
      "title": "Taxa de Execução de Tarefas por Nome (min)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum(rate(celery_tasks_total[1m])) by (task_name)",
          "legendFormat": "Tarefa: {{task_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "id": 2,
      "title": "Taxa de Execução de Tarefas por Status (min)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum(rate(celery_tasks_total[1m])) by (status)",
          "legendFormat": "{{status == 'success' ? 'Concluídas' : 'Com falha'}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 3,
      "title": "Falhas de Tarefas (min)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 7, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(celery_tasks_total{status=\"failure\"}[1m]))",
          "legendFormat": "Falhas/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "red", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value"
      }
    },
    {
      "id": 4,
      "title": "Numero de Workers Ativos",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 6, "y": 7, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(celery_workers_total)",
          "legendFormat": "Workers Ativos",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 10,
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "orange", "value": 20 },
              { "color": "green", "value": 80}
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 5,
      "title": "Tarefas Pendentes na Fila",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 7, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(celery_queue_length)",
          "legendFormat": "Pendentes",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 50,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },

    {
      "id": 6,
      "title": "Concorrência Total por Worker",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 18, "y": 7, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(celery_worker_concurrency)",
          "legendFormat": "Threads",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 20,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "orange", "value": 10 },
              { "color": "red", "value": 15 }
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 7,
      "title": "Heatmap de Latência de Tarefas (min)",
      "type": "heatmap",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 11, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(celery_task_duration_seconds_bucket[5m])) by (le, task_name)",
          "format": "heatmap",
          "legendFormat": "Tarefa: {{task_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" },
        "heatmap": {
          "maxBucket": "0.01",
          "minBucket": "5"
        }
      }
    },
    {
      "id": 8,
      "title": "Histograma de Latência de Tarefas (P99)",
      "type": "bargauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 10, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(celery_task_duration_seconds_bucket[5m])) by (le, task_name))",
          "legendFormat": "Tarefa: {{task_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "nullValueMode": "connected"
        }
      },
      "options": {
        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    }
  ],
  "time": { "from": "now-15m", "to": "now" },
  "annotations": { "list": [] },
  "tags": ["celery", "marketalert"],
  "templating": { "list": [] },
  "links": []
}
