    },
    {
      "id": 8,
      "title": "Requisições em Andamento",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(scraper_in_flight_requests)",
          "legendFormat": "In flight",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "min": 0, "max": 50 }
      },
      "options": {
        "orientation": "horizontal",
        "showThresholdMarkers": true,
        "showThresholdLabels": false
      }
    },
    {
      "id": 9,
      "title": "Retries por Minuto",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 18, "y": 4, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(scraper_retry_total[1m]))",
