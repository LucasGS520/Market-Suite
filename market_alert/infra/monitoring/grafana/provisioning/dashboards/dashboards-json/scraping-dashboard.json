{
  "uid": "marketalert_scraping_latency",
  "title": "Métricas de Scraping – MarketAlert",
  "schemaVersion": 39,
  "version": 8,
  "refresh": "15s",
  "timezone": "",
  "panels": [
    {
      "id": 1,
      "title": "Total de Scrapes por Minuto",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 5 },
      "targets": [
        {
          "expr": "sum(rate(scraper_requests_total[1m]))",
          "legendFormat": "Scrapes/min",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": true }
      }
    },
    {
      "id": 2,
      "title": "Circuit Breaker Aberto (0 ou 1)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 5, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "max(scraper_circuit_open)",
          "legendFormat": "Circuito Aberto",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": false },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "value": null, "color": "green" },
            { "value": 1, "color": "red" }
          ]
        }
      }
    },
    {
      "id": 3,
      "title": "Latência Média do Scraper (s)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 9 },
      "targets": [
        {
          "expr": "sum(rate(scraping_latency_seconds_sum[5m])) / sum(rate(scraping_latency_seconds_count[5m]))",
          "legendFormat": "Média (s)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 4,
      "title": "Latência P99 do Scraper (s)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 9 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(scraping_latency_seconds_bucket[5m])) by (le, source))",
          "legendFormat": "Fonte: {{source}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 5,
      "title": "Distribuição de Latência do Scraper",
      "type": "heatmap",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 9, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(scraping_latency_seconds_bucket[5m])) by (le, source)",
          "format": "heatmap",
          "legendFormat": "Fonte: {{source}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "nullValueMode": "connected"
        }
      },
      "options": {
        "legend": { "showLegend": true, "displayMode": "list", "placement": "bottom" },
        "heatmap": { "minBucket": "0.01", "maxBucket": "10" }
      }
    },





    {
      "id": 6,
      "title": "Bloqueios HTTP (403/429) por Minuto",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 9, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(scraper_http_blocked_total[1m]))",
          "legendFormat": "Bloqueios HTTP/min",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "value_and_name",
        "colorMode": "value",
        "sparkline": { "show": true }
      }
    },
    {
      "id": 7,
      "title": "CAPTCHAs Detectados por Minuto",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 13, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(scraper_captcha_total[1m]))",
